<script>
var $ = jQuery;

// Load newsletter editions
jQuery.ajax({url: '/sph_newsletter/editions', success: function(result){
    //console.log(result);
    if(Object.keys(result)) {
      $.each(result, function(i, field){
        var item = '<li role="presentation">' +
                      '<div class="checkbox">' +
                        '<input type="checkbox" value="'+field['optin_flag']+'" name="edition" id="checkbox_'+field['optin_flag']+'">' +
