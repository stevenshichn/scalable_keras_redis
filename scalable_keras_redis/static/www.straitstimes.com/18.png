                    '</li>';
        
        $('.editions').append(item);
      });
    }
}});

$('body').on('click', '.nl .checkbox', function(e){
  e.stopPropagation();    
});

$('.nl #button').click(function(){
    var thisEmail = $('.nl #email').val();
    var thisEdition = $('.nl input[name=edition]:checked');
    var result = $('.nl .result');
    var error = 0;
    var counter = 5;
    
    if (!isValidEmail(thisEmail)){
        error++;
       $('.nl #email').css('cssText', 'border: 1px solid #f00 !important; color: #f00');
    }
    if (thisEdition.length == 0){
        error++;
        $('.nl #menu1').css('cssText', 'color: #ff0000;');
    }

    if(error==0 ) {
        $('.nl .dropdown, .nl .email-field').css('display','none');
        result.css('display','block');

        var nl_edition = [];
        thisEdition.each( function() {
            nl_edition.push($(this).val());
        });

        var params = {
            nl_email:thisEmail,
            nl_edition:nl_edition
        }

        $.post('/sph_newsletter/ajax/subscribe', params, function( data ) {
            var response = JSON.parse(data);
            if(response.status == 'SUCCESS') {
                //console.log('Success');
                result.text('Subscription successful!')
